.container
  .content
    %h2 Events
    %table.table.table-striped
      %thead
        %tr
          %th Timestamp
          %th Type
          %th Message
        %tbody
      ~@events.each do |event|
        %tr
          %td 
            %a{:href => '/event/' << event['_id'].to_s}=  event['_type']
          
